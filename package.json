{
  "name": "rsbuild-react-ts",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rsbuild build",
    "dev": "rsbuild dev",
    "format:check": "prettier --check ./src/**/*.{ts,tsx,scss}",
